<?xml version="1.0" encoding="utf-8"?>
<package>
  <metadata>
    <id>Insight.Database.Compatibility3x</id>
    <version>4.0</version>
    <title>Insight.Database.Compatibility3x</title>
    <authors>Jon Wagner</authors>
    <description>
Allows code written against Insight.Database 3.x to run with the Insight.Database 4.x library
with almost no changes. You only need this if you use certain deprecated features and don't have
time to make minor changes.
	</description>
    <language>en-US</language>
    <projectUrl>http://github.com/jonwagner/Insight.Database</projectUrl>
    <licenseUrl>http://opensource.org/licenses/ms-pl.html</licenseUrl>
	<iconUrl>https://github.com/jonwagner/Insight.Database/raw/master/InsightDatabase.png</iconUrl>
    <tags>Insight orm micro-orm</tags>
    <releaseNotes>See important release notes at: https://github.com/jonwagner/Insight.Database/wiki/Change-Log</releaseNotes>
	<dependencies>
		<dependency id="Insight.Database" version="4.1"></dependency>
	</dependencies>
	  <frameworkAssemblies>
		  <frameworkAssembly assemblyName="Microsoft.CSharp" targetFramework="net40,net45" />
	  </frameworkAssemblies>
  </metadata>
  <files>
    <file src="..\Insight.Database\bin\NET45\Insight.Database.Compatibility3x.dll" target="lib\NET45" />
    <file src="..\Insight.Database\bin\NET45\Insight.Database.Compatibility3x.xml" target="lib\NET45" />
    <file src="..\Insight.Database\bin\NET45\Insight.Database.Compatibility3x.pdb" target="lib\NET45" />
